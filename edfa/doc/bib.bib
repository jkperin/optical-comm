	doi = {10.1109/JLT.1986.1074698},
	file = {:D$\backslash$:/Jose/Dropbox/research/coherent/analog/(Kazovsky) Balanced PLL for optical Homodyne Receivers - Performance analysis, Design considerations, and Laser Linewidth Requirements.pdf:pdf},
