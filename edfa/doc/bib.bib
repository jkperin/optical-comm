	mendeley-groups = {Coherent/Analog},
	number = {2},
	pages = {182--195},
	title = {{Balanced Phase-Locked Loops for Optical Homodyne Receivers: Performance Analysis, Design Considerations, and Laser Linewidth Requirements}},
	volume = {4},
